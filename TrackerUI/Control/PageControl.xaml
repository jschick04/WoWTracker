<UserControl x:Class="TrackerUI.Control.PageControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:TrackerUI.Control"
    xmlns:attachedProperties="clr-namespace:TrackerUI.AttachedProperties"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800">

    <Grid>
        <ContentControl x:Name="OldPage" attachedProperties:NoFrameHistory.Value="True" />
        <ContentControl x:Name="NewPage" attachedProperties:NoFrameHistory.Value="True" />
    </Grid>

</UserControl>